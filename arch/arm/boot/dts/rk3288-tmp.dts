/dts-v1/;
#include "rk3288.dtsi"

/ {
	compatible = "rockchip,rk3288-tmp", "rockchip,rk3288";

	aliases {
		mshc0 = &sdmmc;
		mshc1 = &sdio0;
		mshc3 = &emmc;

		serial0 = &uart0;
		serial1 = &uart1;
		serial2 = &uart2;
		serial3 = &uart3;
		serial4 = &uart4;
	};

	memory {
		reg = <0x0 0x80000000>;
	};

	regulators {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		vcc_sd0: fixed-regulator@0 {
			compatible = "regulator-fixed";
			reg = <0>;

			regulator-name = "sdmmc-supply";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			gpio = <&gpio7 11 GPIO_ACTIVE_LOW>;
			startup-delay-us = <100000>;
//			vin-supply = <&vcc_io>;
		};
	};
	/*for lvds 
	panel:panel {
		compatible = "rockchip,panel";
		status = "okay";
		rockchip,output-face = "r6g6b6";

		enable-gpios = <&gpio7 3 GPIO_ACTIVE_HIGH
				&gpio7 4 GPIO_ACTIVE_HIGH
				&gpio7 0 GPIO_ACTIVE_HIGH
				&gpio7 2 GPIO_ACTIVE_HIGH>;
		output-dither;

		display-timings {
			native-mode = <&timing0>;
			timing0: 1280x800 {
				clock-frequency = <71000000>;
				hactive = <1280>;
				vactive = <800>;
				hback-porch = <100>;
				hfront-porch = <18>;
				vback-porch = <8>;
				vfront-porch = <6>;
				hsync-len = <10>;
				vsync-len = <2>;
				hsync-active = <0>;
				vsync-active = <0>;
				de-active = <0>;
				pixelclk-active = <0>;
			};
		};
	};*/
	// for 1536x2048 edp
	panel:panel {
		compatible = "rockchip,panel";
		status = "okay";
		rockchip,output-face = "r6g6b6";

		enable-gpios = <&gpio7 3 GPIO_ACTIVE_HIGH>;
		output-dither;

		display-timings {
			native-mode = <&timing1>;
			timing1: timing@1 {
				clock-frequency = <205000000>;
				hactive = <1536>;
				vactive = <2048>;
				hback-porch = <48>;
				hfront-porch = <12>;
				vback-porch = <8>;
				vfront-porch = <8>;
				hsync-len = <16>;
				vsync-len = <4>;
				hsync-active = <0>;
				vsync-active = <0>;
				de-active = <0>;
				pixelclk-active = <1>;
			};
		};
	};

	soc {
		gpio-keys {
			compatible = "gpio-keys";
			#address-cells = <1>;
			#size-cells = <0>;
			autorepeat;

			button@0 {
				gpios = <&gpio0 5 GPIO_ACTIVE_HIGH>;
				linux,code = <116>;
				label = "GPIO Key Power";
				linux,input-type = <1>;
				gpio-key,wakeup = <1>;
				debounce-interval = <100>;
			};
		};

		i2c0: i2c@ff650000 {
			pinctrl-names = "default";
			pinctrl-0 = <&i2c0_xfer>;
			status = "okay";

			rk808: rk808@1b {
				compatible = "rockchip,rk808";
				reg = <0x1b>;

				interrupt-parent = <&gpio0>;
				interrupts = <4 IRQ_TYPE_EDGE_FALLING>; /* gpio0_a */
				gpios = <&gpio0 4 GPIO_ACTIVE_HIGH>,<&gpio0 11 GPIO_ACTIVE_LOW>;
			};
		};
		emmc: dwmmc@ff0f0000 {
			num-slots = <1>;
			status = "okay";
		
			clock-frequency = <150000000>;
			clock-freq-min-max = <400000 150000000>;

			default_voltage = <1>; //1.8v
	
			broken-cd;
			non-removable;
			supports-highspeed;
			caps2-mmc-hs200-1_8v;

			pinctrl-names = "default";
			pinctrl-0 = <&emmc_clk &emmc_cmd &emmc_rst &emmc_pwr &emmc_bus8>;

			slot@0 {
				reg = <0>;
				bus-width = <8>;
				disable-wp;
			};
}		;

		sdmmc: dwmmc@ff0c0000 {
			num-slots = <1>;
			status = "okay";

			clock-frequency = <50000000>;
			clock-freq-min-max = <400000 50000000>;

			supports-highspeed;

			pinctrl-names = "default";
			pinctrl-0 = <&sdmmc_clk &sdmmc_cmd &sdmmc_cd &sdmmc_bus4>;

			slot@0 {
				reg = <0>;
				bus-width = <4>;
				disable-wp;
			};
		};

		sdio0: dwmmc@ff0d0000 {
			num-slots = <1>;
			status = "okay";

			clock-frequency = <50000000>;
			clock-freq-min-max = <400000 50000000>;

			default_voltage = <1>; //1.8v

			broken-cd;
			non-removable;
			cap-sdio-irq;
			supports-highspeed;
			ignore-pm-notify;
			keep-power-in-suspend;

			pinctrl-names = "default";
			pinctrl-0 = <&sdio0_clk &sdio0_cmd &sdio0_cd &sdio0_pwr &sdio0_bkpwr &sdio0_bus4>;

			rockchip,grf = <&grf>;

			slot@0 {
				reg = <0>;
				bus-width = <4>;
				disable-wp;
			};
		};
	
		edp: edp@ff970000 {                                                                                                        
			status = "disabled";
			rockchip,panel = <&panel>;
		};

		lvds: lvds@ff96c000 {
			status = "okay";
			rockchip,data-mapping = "jeida";
			rockchip,data-width = <24>;
			rockchip,panel = <&panel>;
		};

		lcdc1: lcdc@ff940000 {
			status = "okay";
		};
	
		watchdog@ff800000 {
			status = "okay";
		};

		/* uart_bt */
		serial@ff180000 {
			status = "okay";
		};

		/* uart_bb */
		serial@ff190000 {
			status = "okay";
		};

		/* uart_dbg */
		uart2: serial@ff690000 {
			status = "okay";
		};

		/* uart_gps */
		uart3: serial@ff1b0000 {
			status = "okay";
		};

		/* uart_exp */
		uart4: serial@ff1c0000 {
			status = "okay";
		};
	};
};
	&rk808 {
	//compatible = "rockchip,rk808";
	rk808,system-power-controller;
	regulators {
		#address-cells = <1>;
		#size-cells = <0>;

		rk808_dcdc1_reg: regulator@0 {
			reg = <0>;
			regulator-compatible = "DCDC_REG1";
			regulator-name = "vdd_arm";
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;
			regulator-initial-mode = <0x2>;
			regulator-always-on;
			regulator-boot-on;
			regulator-initial-state = <3>;
			regulator-state-mem {
				regulator-state-mode = <0x2>;
				regulator-state-disabled;//disabled
				regulator-state-uv = <900000>;
			};
		};

		rk808_dcdc2_reg: regulator@1 {
			reg = <1>;
			regulator-compatible = "DCDC_REG2";
			regulator-name = "vdd_gpu";
			regulator-min-microvolt = <1100000>;
			regulator-max-microvolt = <1100000>;
			regulator-initial-mode = <0x2>;
			regulator-always-on;
			regulator-boot-on;
			regulator-initial-state = <3>;
			regulator-state-mem {
				regulator-state-mode = <0x2>;
				regulator-state-disabled;
				regulator-state-uv = <900000>;
			};
		};

		rk808_dcdc3_reg: regulator@2 {
			reg = <2>;
			regulator-compatible = "DCDC_REG3";
			regulator-name = "vdd_ddr";
			regulator-initial-mode = <0x2>;
			regulator-always-on;
			regulator-boot-on;
			regulator-initial-state = <3>;
			regulator-state-mem {
				regulator-state-enabled;
			};
		};

		rk808_dcdc4_reg: regulator@3 {
			reg = <3>;
			regulator-compatible = "DCDC_REG4";
			regulator-name = "vccio";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-initial-mode = <0x2>;
			regulator-always-on;
			regulator-boot-on;
			regulator-initial-state = <3>;
			regulator-state-mem {
				regulator-state-mode = <0x2>;
				regulator-state-enabled;
				regulator-state-uv = <2800000>;
			};
		};

		rk808_ldo1_reg: regulator@4 {
			reg = <4>;
			regulator-compatible = "LDO_REG1";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
			regulator-boot-on;
			regulator-initial-state = <3>;
			regulator-state-mem {
				regulator-state-enabled;
				regulator-state-uv = <3300000>;
			};
		};

		rk808_ldo2_reg: regulator@5 {
			reg = <5>;
			regulator-compatible = "LDO_REG2";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
			regulator-boot-on;
			regulator-initial-state = <3>;
			regulator-state-mem {
				regulator-state-enabled;
				regulator-state-uv = <3300000>;
			};
		};

		rk808_ldo3_reg: regulator@6 {
			reg = <6>;
			regulator-compatible = "LDO_REG3";
			regulator-min-microvolt = <1000000>;
			regulator-max-microvolt = <1000000>;
			regulator-always-on;
			regulator-boot-on;
			regulator-initial-state = <3>;
			regulator-state-mem {
				regulator-state-enabled;
				regulator-state-uv = <1000000>;
			};
		};

		rk808_ldo4_reg: regulator@7{
			reg = <7>;
			regulator-compatible = "LDO_REG4";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			regulator-always-on;
			regulator-boot-on;
			regulator-initial-state = <3>;
			regulator-state-mem {
				regulator-state-disabled;
				regulator-state-uv = <1800000>;
			};
		};

		rk808_ldo5_reg: regulator@8{
			reg = <8>;
			regulator-compatible = "LDO_REG5";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <3300000>;
			regulator-initial-state = <3>;
			regulator-always-on;
			regulator-boot-on;
			regulator-state-mem {
				regulator-state-enabled;
				regulator-state-uv = <3300000>;
			};
		};

		rk808_ldo6_reg: regulator@9{
			reg = <9>;
			regulator-compatible = "LDO_REG6";
			regulator-min-microvolt = <1000000>;
			regulator-max-microvolt = <1000000>;
			regulator-initial-state = <3>;
			regulator-always-on;
			regulator-boot-on;
			regulator-state-mem {
				regulator-state-disabled;
				regulator-state-uv = <1000000>;
			};
		};

		rk808_ldo7_reg: regulator@10 {
			reg = <10>;
			regulator-compatible = "LDO_REG7";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			regulator-initial-state = <3>;
			regulator-always-on;
			regulator-boot-on;
			regulator-state-mem {
				regulator-state-enabled;
				regulator-state-uv = <1800000>;
			};
		};

		rk808_ldo8_reg: regulator@11{
			reg = <11>;
			regulator-compatible = "LDO_REG8";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-initial-state = <3>;
			regulator-always-on;
			regulator-boot-on;
			regulator-state-mem {
				regulator-state-enabled;
				regulator-state-uv = <3300000>;
			};
		};

		rk808_ldo9_reg: regulator@12{
			reg = <12>;
			regulator-compatible = "SWITCH_REG1";
			regulator-initial-state = <3>;
			regulator-always-on;
			regulator-boot-on;
			regulator-state-mem {
				regulator-state-enabled;
			};
		};

		rk808_ldo10_reg: regulator@13{
			reg = <13>;
			regulator-compatible = "SWITCH_REG2";
			regulator-initial-state = <3>;
			regulator-always-on;
			regulator-boot-on;
			regulator-state-mem {
				regulator-state-disabled;
			};
		};

	};
};
