	// for 1536x2048 edp
	panel:panel {
		compatible = "rockchip,panel";
		status = "okay";
		rockchip,output-face = "r6g6b6";

		enable-gpios = <&gpio7 3 GPIO_ACTIVE_HIGH>;
		output-dither;

		display-timings {
			native-mode = <&timing1>;
			timing1: timing@1 {
				clock-frequency = <205000000>;
				hactive = <1536>;
				vactive = <2048>;
				hback-porch = <48>;
				hfront-porch = <12>;
				vback-porch = <8>;
				vfront-porch = <8>;
				hsync-len = <16>;
				vsync-len = <4>;
				hsync-active = <0>;
				vsync-active = <0>;
				de-active = <0>;
				pixelclk-active = <1>;
			};
		};
	};

	soc {
		gpio-keys {
			compatible = "gpio-keys";
			#address-cells = <1>;
			#size-cells = <0>;
			autorepeat;

			button@0 {
				gpios = <&gpio0 5 GPIO_ACTIVE_HIGH>;
				linux,code = <116>;
				label = "GPIO Key Power";
				linux,input-type = <1>;
				gpio-key,wakeup = <1>;
				debounce-interval = <100>;
			};
		};

		i2c0: i2c@ff650000 {
			pinctrl-names = "default";
			pinctrl-0 = <&i2c0_xfer>;
			status = "okay";
			rk808: rk808@1b {
				compatible = "rockchip,rk808";
				reg = <0x1b>;
				interrupt-parent = <&gpio0>;
				interrupts = <4 IRQ_TYPE_EDGE_FALLING>; /* gpio0_a */
				gpios = <&gpio0 4 GPIO_ACTIVE_HIGH>,<&gpio0 11 GPIO_ACTIVE_LOW>;
			};
		};

		i2c1: i2c@ff140000 {
			pinctrl-names = "default";
			pinctrl-0 = <&i2c1_xfer>;
			status = "okay";
			tpm: tpm@20 {
				compatible = "infineon,slb9635tt";
				reg = <0x20>;
			};
		};

		edp: edp@ff970000 {                                                                                                        
			status = "okay";
			rockchip,panel = <&panel>;
		};

		lvds: lvds@ff96c000 {
			status = "disabled";
			rockchip,data-mapping = "jeida";
			rockchip,data-width = <24>;
		      rockchip,panel = <&panel>;
		};

		lcdc1: lcdc@ff940000 {
			status = "okay";
		 };
	
		watchdog@ff800000 {
			status = "okay";
		};

		pwm0: pwm@ff680000 {
			status = "okay";
		};
		
		serial@ff180000 {
			status = "okay";
		};

		serial@ff190000 {
			status = "okay";
		};

		uart2: serial@ff690000 {
			pinctrl-names = "default";
			pinctrl-0 = <&uart2_xfer>;
			status = "okay";
		};

		uart3: serial@ff1b0000 {
			status = "okay";
		};

		uart4: serial@ff1c0000 {
			status = "okay";
		};
	};
};

&emmc {
	num-slots = <1>;
	status = "okay";

	clock-frequency = <150000000>;
	clock-freq-min-max = <400000 150000000>;

	default_voltage = <1>; //1.8v

	broken-cd;
	non-removable;
	supports-highspeed;
	//caps2-mmc-hs200-1_8v;

	pinctrl-names = "default";
	pinctrl-0 = <&emmc_clk &emmc_cmd &emmc_rst &emmc_pwr &emmc_bus8>;

	slot@0 {
		reg = <0>;
		bus-width = <8>;
		disable-wp;
	};
};

&sdmmc {
	num-slots = <1>;
	status = "okay";

	clock-frequency = <50000000>;
	clock-freq-min-max = <400000 50000000>;

	supports-highspeed;

	pinctrl-names = "default";
	pinctrl-0 = <&sdmmc_clk &sdmmc_cmd &sdmmc_cd &sdmmc_bus4>;

	slot@0 {
		reg = <0>;
		bus-width = <4>;
		disable-wp;
	};
};

&sdio0 {
	num-slots = <1>;
	status = "okay";

	clock-frequency = <50000000>;
	clock-freq-min-max = <400000 50000000>;

	default_voltage = <1>; //1.8v

	broken-cd;
	non-removable;
	cap-sdio-irq;
	supports-highspeed;
	ignore-pm-notify;
	keep-power-in-suspend;

	pinctrl-names = "default";
	pinctrl-0 = <&sdio0_clk &sdio0_cmd &sdio0_cd &sdio0_pwr &sdio0_bkpwr &sdio0_bus4>;

	rockchip,grf = <&grf>;

	slot@0 {
		reg = <0>;
		bus-width = <4>;
		disable-wp;
	};
};

&spi0 {
	status = "okay";
	cros_ec: cros-ec@0 {
		compatible = "google,cros-ec-spi";
		spi-max-frequency = <4000000>;
		interrupt-parent = <&gpio7>;
		interrupts = <7 IRQ_TYPE_LEVEL_LOW>;
		reg = <0>;

		google,cros-ec-spi-msg-delay = <2000>;

		charger: gpio-charger@0 {
			compatible = "gpio-charger";
			charger-type = "usb-sdp";
			gpios = <&gpio0 8 GPIO_ACTIVE_HIGH>;
		};

		i2c-tunnel {
			compatible = "google,cros-ec-i2c-tunnel";
			#address-cells = <1>;
			#size-cells = <0>;

			google,remote-bus = <0>;

			battery: sbs-battery@b {
				compatible = "sbs,sbs-battery";
				reg = <0xb>;
				sbs,i2c-retry-count = <2>;
				sbs,poll-retry-count = <1>;
			};
		};
	};
};

&spi2 {
	status = "okay";
};

&usb_otg {
	status = "okay";
};

&usb_host1 {
	status = "okay";
};

&usb_host0_ehci {
	status = "okay";
};
#include "cros-ec-keyboard.dtsi"
