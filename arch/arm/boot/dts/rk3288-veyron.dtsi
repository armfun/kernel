	aliases {
		mshc0 = &sdmmc;
		mshc1 = &sdio0;
		mshc3 = &emmc;

		serial0 = &uart0;
		serial1 = &uart1;
		serial2 = &uart2;
		serial3 = &uart3;
		serial4 = &uart4;
	};

	memory {
		reg = <0x0 0x80000000>;
	};
    
